''''''
import time
score = int(input('점수를 입력하세요 : '))
if score >= 90:
    grade = 'A'
elif score >= 80:
    grade = 'B'
elif score >= 70:
    grade = 'C'
elif score >= 60:
    grade = 'D'
else:
    grade = 'F'
print('당신의 등급은? :{} '.format(grade))
''''''


N = int(input('양의 정수를 입력하세요: '))
s = 0
start = time.time()
for i in range(1, N+1):
    s += i
end = time.time()
print(s, end-start)
